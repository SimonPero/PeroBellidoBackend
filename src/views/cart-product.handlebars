<div>
{{#if cart.products.length}}
<h1>Detalles del carrito</h1>
  <section class="container">
  <ul>
    {{#each cart.products}}
    <li>
      <p>Producto: {{this.idProduct.title}}</p>
      <p>Descripci√≥n: {{this.idProduct.description}}</p>
      <p>Precio: {{this.idProduct.price}}</p>
